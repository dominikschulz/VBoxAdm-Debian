#!/usr/bin/perl
use lib qw(../lib);

use VBoxAdm::Frontend '@VERSION@';

binmode STDIN,  ":utf8";
binmode STDOUT, ":utf8";
binmode STDERR, ":utf8";

my $vbox = VBoxAdm::Frontend->new(
    PARAMS => {
        'require_ssl'    => 1,
        'rewrite_to_ssl' => 1,
    }
);
$vbox->run();

__END__

=head1 NAME

